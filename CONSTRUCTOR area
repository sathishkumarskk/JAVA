public class area{
    
    public area(int a,int b){
        int area=a*b;
        int p=2*(a+b);
        System.out.println("Area of rectangle:"+area);
        System.out.print("Perimeter of rectangle:"+p);
        
    }
    public static void main(String[] argv){
        area ob= new area(50,30);
    }    
    
}
