class employe{
        String name;
        int id;
        double salary;
    public void fun1(String s,int i,double d){
        name=s;
        id=i;
        salary=d;        
    }    
}
class manager extends employe{
    int team_size;
        public void fun1(String s,int i,double d,int t){
        super.fun1(s,i,d);
        int team_size=t;
             System.out.println("MANAGER");
        System.out.println("name:"+name);
        System.out.println("id:"+id);
        System.out.println("salary:"+salary);
        System.out.println("team size:"+team_size);
        
        }
        public static void main(String[] argv){
            manager o=new manager();
            o.fun1("sathish",123,100000.00,4);
        }
} 
public class developer extends employe{
    String programing_language;
        public void fun1(String s,int i,double d,String p){
        super.fun1(s,i,d);
        programing_language=p;
                System.out.println("DEVELOPER");
        System.out.println("name:"+name);
        System.out.println("id:"+id);
        System.out.println("salary:"+salary);
        System.out.println("programing language:"+programing_language);
        }
        public static void main(String[] argv){
            developer ob=new developer();
            ob.fun1("sathish",123,100000,"java");
        }
}
