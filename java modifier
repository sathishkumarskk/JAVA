class per{
    private String name;
    public void setname(String a){
        name=a;
    }
    public void getname(){
        System.out.print(name);
    }    
}
public class data{
    public static void main(String[] argv){
        per ob=new per();
        ob.setname("sathish");
        ob.getname();
    }
}
